datasource db {
  provider = "postgresql"
  env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model jobs {
  id          Int       @id @default(autoincrement())
  title       String
  company     String
  location    String?
  job_type    String?
  salary_min  Int?
  salary_max  Int?
  deadline    DateTime?
  description String?
}
